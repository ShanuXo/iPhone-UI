{
    "manifest_version": 2,
    "name": "Your Extension Name",
    "version": "1.0",
    "description": "Your extension description",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"], 
        "js": ["contentScript.js"]
      }
    ],
    "web_accessible_resources": [
      "src/index.css"
    ],
    "permissions": [
      "tabs",
      "activeTab"
    ]
  }
  